<script type="text/javascript">
  window.onload = function () {
    var chart = new CanvasJS.Chart("chartContainer",
    {
      axisY: {
        title: "Products Sold"
      },
      axisX: {
        title: "Months"
      },
      data: [
      {        
        color: "#b2d732",
        type: "column",  
        dataPoints: [      
        { x: 1, y: 40, label: "January"},
        { x: 2, y: 60,  label: "February" },
        { x: 3, y: 50,  label: "March"},
        { x: 4, y: 60,  label: "April"},
        { x: 5, y: 70,  label: "May"},
        { x: 6, y: 100, label: "June"},
        { x: 7, y: 120,  label: "July"},        
        { x: 8, y: 110,  label: "August"},
        { x: 9, y: 130,  label: "September"},
        { x: 10, y: 140,  label: "October"},
        { x: 11, y: 150,  label: "November"},
        { x: 12, y: 190,  label: "Decemeber"}
        ]
      }
      ]
    });

    chart.render();
  }
  </script>
 